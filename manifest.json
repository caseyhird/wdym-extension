{
  "manifest_version": 3,
  "name": "wdym?",
  "description": "Extension to ask the question 'wdym?'",
  "version": "1.0.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "dialogue.html"
  },
  "icons": {
    "16": "icons/wdym_logo_16.png",
    "24": "icons/wdym_logo_24.png",
    "32": "icons/wdym_logo_32.png",
    "48": "icons/wdym_logo_48.png",
    "128": "icons/wdym_logo_128.png"
  }
}